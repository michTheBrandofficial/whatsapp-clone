{
  "compilerOptions": {
    "paths": {
      "@assets/*": ["./assets/*"],
      "@utils/*": ["./utils/*"]
    },
    "resolveJsonModule": true,
    "baseUrl": ".",
    "jsx": "react-jsxdev",
    "jsxImportSource": "nixix/types",
    "typeRoots": [
      "nixix",
      "nixix/types"
    ],
    "module": "es2022",
    "target": "es2022",
    "checkJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
  },
  "include": ["**/*"]
}